const API_KEY = 'x2bHM1bI70WtVcjS5zwAZnQOx1w2VHUqOvpE4HEM';
var Congress = require('propublica-congress-node');
var client = new Congress(API_KEY);

function getMembers(chamber) {
  client.memberLists({
    chamber: chamber
  }).then(function (res) {
    return res;
  })
}

console.log(getMembers('house'));
